package main;

use strict;
use warnings;
use GDBM_File;
use MLDBM qw(GDBM_File Storable);
use Data::Dumper;

my %db;

tie %db, 'MLDBM', "/var/lib/wanna-build/build-db-unstable", &GDBM_WRCREAT, 0664
    or die "FATAL: Cannot open database: $!\n";

print Dumper(\%db);

exit 0;

